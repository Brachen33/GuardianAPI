{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:58604",
      "sslPort": 44384
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/values",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "GuardianAPI": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/values",

      // Local host
       "applicationUrl": "https://localhost:5001;http://localhost:5000",

      // Local IIS
      // "applicationUrl": "https://local.guardianapi.com;http://local.guardianapi.com",

      // Dev IIS
      //  "applicationUrl": "https://dev.guardianapi.com;http://dev.guardianapi.com",

      // Demo IIS
     // "applicationUrl": "https://demo.guardianapi.com;http://demo.guardianapi.com",

      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}